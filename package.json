{
	"name": "code-decompiler",
	"displayName": "code-decompiler",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.68.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:decfs",
		"onCommand:code-decompiler.decompile"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "code-decompiler.decompile",
				"title": "Decompile",
				"category": "Decompiler"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "true",
					"command": "code-decompiler.decompile",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "python3 -m grpc_tools.protoc --grpc_python_out=server/ --python_out=server/ -Iproto proto/server/proto/*.proto && protoc --plugin=./node_modules/.bin/protoc-gen-ts --ts_out=./src/proto -Iproto proto/server/proto/*.proto && tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/google-protobuf": "^3.15.6",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/vscode": "^1.68.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"grpc_tools_node_protoc_ts": "^5.3.2",
		"mocha": "^10.0.0",
		"protoc-gen-ts": "^0.8.4",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"@grpc/grpc-js": "^1.6.7",
		"google-protobuf": "^3.20.1",
		"grpc-tools": "^1.11.2",
		"vscode-languageclient": "^8.0.1"
	}
}